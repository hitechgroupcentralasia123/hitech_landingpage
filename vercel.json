{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "hitech/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/hitech-landing/build/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/hitech-landing/build/index.html"
    },    
    {
      "src": "/studio(.*)",
      "dest": "/hitech/dist/$1"
    }
  ]
}
