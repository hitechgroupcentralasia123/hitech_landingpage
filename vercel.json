{
  "builds": [
    {
      "src": "hitech/package.json",
      "use": "@vercel/static-build",
      "distDir": "dist"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/structure(.*)",
      "dest": "/hitech/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ]
}
