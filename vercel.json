{
  "builds": [
    {
      "src": "hitech/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "hitech/dist"
      }
    },
    {
      "src": "hitech/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "hitech/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/hitech/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/hitech/build/$1"
    }
  ]
}
