{
  "builds": [
    {
      "src": "hitech_landingpage/hitech/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "hitech_landingpage/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build" 
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/hitech_landingpage/build/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/hitech_landingpage/build/index.html"
    },    
    {
      "src": "/studio(.*)",
      "dest": "/hitech_landingpage/hitech/dist/$1"
    }
  ]
}
